<div class="box box-default">
  <div class="box-body">
    <table id="example2" class="table table-bordered table-hover table-condensed">
      <tbody>
        <tr>
          <th colspan="5">
            <strong><center>Branch Detail</center></strong>
          </th>
        </tr>
        <tr>
          <th>Code</th>
          <td><%= @branch.code %></td>
        </tr>
        <tr>
          <th> Name </th>
          <td><%= @branch.name %></td>
        </tr>
        <tr>
          <th>Address </th>
          <td><%= @branch.address %></td>
        </tr>
        <tr>
          <th>Email </th>
          <td><%= @branch.email %></td>
        </tr>
        <tr>
          <th>Contact No. </th>
          <td><%= @branch.contact_no %></td>
        </tr>
        <tr>
          <th>Status </th>
          <td><%= @branch.status %></td>
        </tr>
      </tbody>
    </table><br>
    <%= link_to 'Edit', edit_branch_path(@branch), class: 'btn btn-default btn-sm fa' %> |
    <%= link_to 'Back', branches_path ,class: 'btn btn-default btn-sm fa fa-arrow-left' %>
  </div>
</div>

