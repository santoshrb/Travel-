<p id="notice"><%= notice %></p>

<h1>Listing File Processes</h1>

<table>
  <thead>
    <tr>
      <th>Vehicle booking</th>
      <th>Financer</th>
      <th>Contact person</th>
      <th>Contact no</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @file_processes.each do |file_process| %>
      <tr>
        <td><%= file_process.vehicle_booking %></td>
        <td><%= file_process.financer %></td>
        <td><%= file_process.contact_person %></td>
        <td><%= file_process.contact_no %></td>
        <td><%= file_process.status %></td>
        <td><%= link_to 'Show', file_process %></td>
        <td><%= link_to 'Edit', edit_file_process_path(file_process) %></td>
        <td><%= link_to 'Destroy', file_process, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New File process', new_file_process_path %>
