<div class="box">
  <div class="box-body">

 <table id="example6" class="table table-bordered table-hover table-condensed">
      <thead>
        <tr>
          <th>Sr.No</th>
          <th>Mobile no</th>
          <th>Name</th>
          <th>Email</th>
          <th>Address</th>
          <th>Place</th>
          <th>Date</th>
        </tr>
      </thead>

      <tbody>
        <% i = 0 %>
        <% @enquiries.each do |enquiry| %>
          <tr>
            <td><%= i = i + 1 %></td>
            <td><%= enquiry.mobile_no %></td>
            <td><%= enquiry.name_first %> <%= enquiry.middle_name %> <%= enquiry.last_name %></td>
            <td><%= enquiry.email %></td>
            <td><%= enquiry.address %></td>
            <td><%= enquiry.place %></td>
            <td><%= enquiry.enquiry_date %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  <br>
    <%= link_to " Excel ",datewise_enquiry_report_enquiries_path(:salary =>{:from => params[:salary][:from],:to => params[:salary][:to]}, format: "xls"), class: "btn btn-xl btn-success fa fa-print" %>
    <%= link_to " PDF ",datewise_enquiry_report_enquiries_path(:salary =>{:from => params[:salary][:from],:to => params[:salary][:to]}, format: "pdf"), class: "btn btn-xl btn-primary fa fa-print",:target => "_blank" %>
  </div>
</div>