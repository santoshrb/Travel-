<div class="ajax-progress"></div>

<table class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Sr.No</th>
      <th>Type</th>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% i = 0 %>
    <% @schemes.each do |scheme| %>
      <tr>
        <td><%= i = i + 1 %></td>
        <td><%= scheme.scheme_type %></td>
        <td><%= scheme.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
