<table id="example5" class="table table-bordered table-hover table-condensed">
      <thead>
        <tr>
          <th>Sr.No</th>
          <th>Mobile no</th>
          <th>Name</th>
          <th>Email</th>
          <th>Address</th>
          <th>Place</th>
          <th>Date</th>
        </tr>
      </thead>

      <tbody>
        <% i = 0 %>
        <% @enquiries.each do |enquiry| %>
          <tr>
            <td><%= i = i + 1 %></td>
            <td><%= enquiry.mobile_no %></td>
            <td><%= enquiry.name_first %> <%= enquiry.middle_name %> <%= enquiry.last_name %></td>
            <td><%= enquiry.email %></td>
            <td><%= enquiry.address %></td>
            <td><%= enquiry.place %></td>
            <td><%= enquiry.enquiry_date %></td>
          </tr>
        <% end %>
      </tbody>
    </table>