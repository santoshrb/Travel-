<div class="box">
  <div class="box-header">
    <h3 class="box-title">Schemes</h3>
    <div class="pull-right">
      <%= link_to 'New Scheme', new_scheme_path, class: 'btn btn-sm btn-primary' %>
      <%#= link_to " Upload ", import_xl_agencies_path, class: 'fa fa-upload btn btn-xl btn-info' %> 
      <%#= link_to " Template", "/Templates/7.Agency.xls", :class=>"fa fa-download btn-xl btn btn-warning" %>
    </div>
  </div><!-- /.box-header -->
  <div class="box-body">
    <table id="example6" class="table table-bordered table-hover table-condensed">
      <thead>
        <tr>
          <th>Sr.No</th>
          <th>Type</th>
          <th>Name</th>
          <th>Budget</th>
          <th>Down Payment</th>
          <th>Status</th>
          <th>Actions</th>
        </tr>
      </thead>

      <tbody>
        <% i = 0 %>
        <% @schemes.each do |scheme| %>
          <tr>
            <td><%= i = i + 1 %></td>
            <td><%= scheme.scheme_type %></td>
            <td><%= scheme.name %></td>
            <td><%= scheme.budget %></td>
            <td><%= scheme.down_payment %></td>
            <td><%= scheme.from_date %></td>
            <td><%= scheme.to_date %></td>
            <td><%= scheme.status %></td>
            <td>
              <%= link_to ' ', scheme, class: 'btn btn-xs btn-default fa fa-eye' %>
              <%= link_to ' ', edit_scheme_path(scheme), class: 'btn btn-xs btn-success fa fa-edit' %>
              <%= link_to ' ', scheme, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-xs btn-danger fa fa-trash' %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
      <%#= link_to 'Back', root_url ,class: 'btn btn-default btn-sm fa fa-arrow-left' %>
  </div>
</div>
